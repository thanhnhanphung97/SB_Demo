@model Demo.Models.DTO.AccountDTO
@{
    ViewBag.Title = "Index";
    ViewBag.UserName = Session["loginSession"];
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section jsFooter{

    <script>
        function mouseinIntroduce() {
            document.getElementById("in").style.width = "24%";
        }
        function mouseoutIntroduce() {
            document.getElementById("in").style.width = "16%";
        }
        
        function mouseinNew() {
            document.getElementById("ne").style.width = "24%";
        }
        function mouseoutNew() {
            document.getElementById("ne").style.width = "16%";
        }
        
        function mouseinPartner() {
            document.getElementById("pa").style.width = "24%";
        }
        function mouseoutPartner() {
            document.getElementById("pa").style.width = "16%";
        }
    </script>

}
